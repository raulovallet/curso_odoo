<odoo>
    
    
    <!--
    
    Human readable name
    Human readable name
    
    Inherited Views
    
    - - - - - - - - - model
    - Model·········: model
    - Base Module···: module_name
    - Mode··········: primary|extension
    - View priority·: 16
    - - - - - - - - - model
    
    -->
    
    <!-- primary|extension Inherited .:Tree:. view for "model" -->
    <record id="view_model_tree_inherited" model="ir.ui.view">
        <field name="name">view.model.tree inherited</field>
        <field name="model">model</field>
        <!-- Please veryfiy, that the following inherited view really sticks to the naming convention. -->
        <field name="inherit_id" ref="module_name.view_model_tree" />
        <field name="type">tree</field>
        <field name="priority" eval="16" />
        <field name="active" eval="True" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                
            </xpath>
        </field>
    </record>
    
    <!-- primary|extension Inherited .:Form:. view for "model" -->
    <record id="view_model_form_inherited" model="ir.ui.view">
        <field name="name">view.model.form inherited</field>
        <field name="model">model</field>
        <!-- Please veryfiy, that the following inherited view really sticks to the naming convention. -->
        <field name="inherit_id" ref="module_name.view_model_form" />
        <field name="type">form</field>
        <field name="priority" eval="16" />
        <field name="active" eval="True" />
        <field name="arch" type="xml">
            
            <xpath expr="//field[@name='name']" position="after">
                
            </xpath>
            
        </field>
    </record>
    
    <!-- primary|extension Inherited .:Search:. view for "model" -->
    <record id="view_model_search_inherited" model="ir.ui.view">
        <field name="name">view.model.search inherited</field>
        <field name="model">model</field>
        <!-- Please veryfiy, that the following inherited view really sticks to the naming convention. -->
        <field name="inherit_id" ref="module_name.view_model_search" />
        <field name="type">search</field>
        <field name="mode">primary|extension</field>
        <field name="priority" eval="16" />
        <field name="active" eval="True" />
        <field name="arch" type="xml">
            <!-- 
            <xpath expr="//field[@name='name']" position="after">
                
            </xpath>
            -->
        </field>
    </record>
    
    
    
</odoo>